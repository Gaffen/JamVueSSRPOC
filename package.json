    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "bluebird": "^3.5.5",
    "browser-sync": "^2.26.7",
    "bs-fullscreen-message": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "co": "^4.6.0",
    "compression": "^1.7.4",
    "css-loader": "^3.2.0",
    "debug": "^4.1.1",
    "file-loader": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "mini-css-extract-plugin": "^0.8.0",
